{
  "name": "k-legrand.com",
  "version": "3.1.1",
  "description": "My website powered by Gatsby ❤️",
  "author": {
    "name": "Kevin Legrand",
    "email": "manoz@outlook.com",
    "url": "https://k-legrand.com"
  },
  "homepage": "https://k-legrand.com",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoz/k-legrand.com"
  },
  "bugs": {
    "url": "https://github.com/Manoz/k-legrand.com/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "yarn": ">=1.22",
    "node": ">=14.5",
    "npm": ">=6.14"
  },
  "scripts": {
    "prebuild": "rimraf ./public && rimraf .cache",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "jest": "jest",
    "lint": "npm-run-all --continue-on-error --parallel lint:js lint:css",
    "lint:css": "stylelint \"./src/**/*.{js,jsx}\"",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm-run-all -s lint jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "gatsby": "^2.24.7",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.3.12",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.19",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.2.19",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^4.25.1",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-source-filesystem": "^2.3.20",
    "gatsby-transformer-sharp": "^2.5.11",
    "gsap": "^3.4.1",
    "moment": "^2.27.0",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0",
    "styled-components": "^5.1.1",
    "tailwindcss": "^1.5.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@hot-loader/react-dom": "^16.13.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "jest": "^26.1.0",
    "jest-cli": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "stylelint-scss": "^3.18.0"
  }
}
