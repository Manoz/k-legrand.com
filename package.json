{
  "name": "k-legrand.com",
  "version": "2.0.5",
  "description": "My website powered by Gatsby ❤️",
  "author": {
    "name": "Kevin Legrand",
    "email": "legrd.kevin@gmail.com",
    "url": "http://k-legrand.com"
  },
  "homepage": "http://k-legrand.com",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoz/k-legrand.com"
  },
  "bugs": {
    "url": "https://github.com/Manoz/k-legrand.com/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "yarn": "^1.9.4",
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "npm-run-all -p format:md format:src",
    "format:md": "prettier --write \"content/**/*.md\" --no-semi",
    "format:src": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "jest": "jest",
    "lint": "npm-run-all --continue-on-error --parallel lint:js lint:css",
    "lint:css": "stylelint \"./src/**/*.{js,jsx}\"",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "open:build": "npm run build && node config/distServer.js",
    "postformat": "npm run lint",
    "prebuild": "rimraf ./public && rimraf .cache",
    "test": "npm-run-all -s lint jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "gatsby": "^2.15.35",
    "gatsby-plugin-catch-links": "^2.1.13",
    "gatsby-plugin-feed": "^2.3.16",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-lodash": "^3.1.11",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-netlify": "^2.1.19",
    "gatsby-plugin-nprogress": "^2.1.10",
    "gatsby-plugin-offline": "^3.0.14",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-plugin-styled-components": "^3.1.9",
    "gatsby-plugin-twitter": "^2.1.10",
    "gatsby-remark-autolink-headers": "^2.1.14",
    "gatsby-remark-copy-linked-files": "^2.1.26",
    "gatsby-remark-images": "^3.1.26",
    "gatsby-remark-prismjs": "^3.3.18",
    "gatsby-remark-responsive-iframe": "^2.2.23",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-remark": "^2.6.28",
    "gatsby-transformer-sharp": "^2.2.21",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prismjs": "^1.17.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-share": "^3.0.1",
    "react-twitter-widgets": "^1.7.1",
    "sanitize.css": "^11.0.0",
    "styled-components": "^4.4.0",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "browser-sync": "^2.26.7",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-plugin-eslint": "^2.0.5",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  }
}
