{
  "name": "k-legrand.com",
  "version": "2.0.4",
  "description": "My website powered by Gatsby ❤️",
  "author": {
    "name": "Kevin Legrand",
    "email": "legrd.kevin@gmail.com",
    "url": "http://k-legrand.com"
  },
  "homepage": "http://k-legrand.com",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoz/k-legrand.com"
  },
  "bugs": {
    "url": "https://github.com/Manoz/k-legrand.com/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "yarn": "^1.9.4",
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "npm-run-all -p format:md format:src",
    "format:md": "prettier --write \"content/**/*.md\" --no-semi",
    "format:src": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "jest": "jest",
    "lint": "npm-run-all --continue-on-error --parallel lint:js lint:css",
    "lint:css": "stylelint './src/**/*.{js,jsx}'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "open:build": "npm run build && node config/distServer.js",
    "postformat": "npm run lint",
    "prebuild": "rimraf ./public && rimraf .cache",
    "test": "npm-run-all -s lint jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "gatsby": "^2.0.40",
    "gatsby-plugin-catch-links": "^2.0.6",
    "gatsby-plugin-feed": "^2.0.9",
    "gatsby-plugin-google-analytics": "^2.0.7",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-lodash": "^3.0.2",
    "gatsby-plugin-manifest": "^2.0.7",
    "gatsby-plugin-netlify": "^2.0.3",
    "gatsby-plugin-nprogress": "^2.0.6",
    "gatsby-plugin-offline": "^2.0.12",
    "gatsby-plugin-react-helmet": "^3.0.1",
    "gatsby-plugin-sharp": "^2.0.12",
    "gatsby-plugin-sitemap": "^2.0.2",
    "gatsby-plugin-styled-components": "^3.0.1",
    "gatsby-plugin-twitter": "^2.0.7",
    "gatsby-remark-autolink-headers": "^2.0.10",
    "gatsby-remark-copy-linked-files": "^2.0.6",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.0.3",
    "gatsby-remark-responsive-iframe": "^2.0.6",
    "gatsby-source-filesystem": "^2.0.7",
    "gatsby-transformer-remark": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.8",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "prismjs": "^1.15.0",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-helmet": "^5.2.0",
    "react-share": "^2.3.1",
    "react-twitter-widgets": "^1.7.1",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "^1.8.0",
    "browser-sync": "^2.26.3",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "gatsby-plugin-eslint": "^2.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.15.1",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "url-join": "^4.0.0"
  }
}
