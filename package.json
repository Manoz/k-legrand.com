{
  "name": "k-legrand.com",
  "version": "2.0.0-beta.2",
  "description": "My website powered by Gatsby v2",
  "author": {
    "name": "Kevin Legrand",
    "email": "legrd.kevin@gmail.com",
    "url": "http://k-legrand.com"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoz/k-legrand.com"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "prebuild": "rimraf ./public && rimraf .cache",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "open:build": "node config/distServer.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-eslint-v2": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-lodash": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-nprogress": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-plugin-twitter": "next",
    "gatsby-remark-autolink-headers": "next",
    "gatsby-remark-copy-linked-files": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-responsive-iframe": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-scrollable-anchor": "^0.6.1",
    "react-share": "^2.2.0",
    "react-twitter-widgets": "^1.7.1",
    "sanitize.css": "^6.0.0",
    "styled-components": "^3.4.2",
    "typography": "^0.16.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
