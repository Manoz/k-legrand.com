{
  "name": "k-legrand.com",
  "version": "2.0.0",
  "description": "My website powered by Gatsby v2",
  "author": {
    "name": "Kevin Legrand",
    "email": "legrd.kevin@gmail.com",
    "url": "http://k-legrand.com"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Manoz/k-legrand.com"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "yarn": "^1.2.1",
    "node": ">=8",
    "npm": ">=3"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "npm-run-all -p format:md format:src",
    "format:md": "prettier --write \"content/**/*.md\" --no-semi",
    "format:src": "prettier-eslint --write \"src/**/*.{js,jsx}\"",
    "jest": "jest",
    "lint": "npm-run-all --continue-on-error --parallel lint:js lint:css",
    "lint:css": "stylelint './src/**/*.{js,jsx}'",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "open:build": "node config/distServer.js",
    "postformat": "npm run lint",
    "prebuild": "rimraf ./public && rimraf .cache",
    "test": "npm-run-all -s lint jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "gatsby": "next",
    "gatsby-plugin-catch-links": "next",
    "gatsby-plugin-eslint-v2": "next",
    "gatsby-plugin-feed": "next",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-lodash": "next",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-nprogress": "next",
    "gatsby-plugin-offline": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-plugin-twitter": "next",
    "gatsby-remark-autolink-headers": "next",
    "gatsby-remark-copy-linked-files": "next",
    "gatsby-remark-images": "next",
    "gatsby-remark-prismjs": "next",
    "gatsby-remark-responsive-iframe": "next",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "gatsby-transformer-sharp": "next",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prismjs": "^1.15.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-scrollable-anchor": "^0.6.1",
    "react-share": "^2.2.0",
    "react-twitter-widgets": "^1.7.1",
    "sanitize.css": "^6.0.0",
    "styled-components": "^3.4.2",
    "typography": "^0.16.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/node": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-rc.1",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.1",
    "@babel/plugin-transform-runtime": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.24.6",
    "dotenv": "^6.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "url-join": "^4.0.0"
  }
}
